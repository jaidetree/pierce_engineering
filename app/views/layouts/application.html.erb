<!DOCTYPE html>
<html>
<head>
  <title><%= @title ? @title + ' : ' : '' %>Pierce Engineering</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag 'fancybox/jquery.fancybox-1.3.4.pack' %>
  <%= stylesheet_link_tag '/javascripts/fancybox/jquery.fancybox-1.3.4.css' %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div id="wrapper">
		<div class="body grid960">
	<%= render "partials/header.erb" %>
	<%= render "partials/sidebar.erb" %>
			<div id="banner"></div><!-- #banner -->
			<div id="main">
	<%= content_tag :p, notice, :id => 'notice' if notice.present? %>
	<%= content_tag :p, alert, :id => 'alert' if alert.present? %>
			<%= yield %>
			</div><!-- #main -->
		</div><!-- .grid960 -->
	<%= render "partials/footer.erb" %>
	</div><!-- #wrapper -->
<script type="text/javascript">

  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-25633572-1']);
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

</script>
</body>
</html>
